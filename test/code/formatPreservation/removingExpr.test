<?php
function doBar(\DateTimeImmutable $date)
{
    'foo';
    $date === null ? 'foo' : 'bar';
    $date ? 'foo' : 'bar';
    !$date ? 'foo' : 'bar';

    $date ?: 'test';
    $date ? $date : 'test';
}
